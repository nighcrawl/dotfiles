"*":
  "atom-beautify":
    js:
      break_chained_methods: true
      jslint_happy: true
  "atom-clock":
    dateFormat: "ddd DD MMM YYYY, HH:mm"
    locale: "fr"
  "atom-ctags":
    GotoSymbolClick: "middle click"
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnSave: true
    "atom-ide-diagnostics-ui":
      showDiagnosticTraces: true
    "atom-ide-find-references":
      defaultLocationForPane: "center"
    "atom-ide-outline-view": {}
    "debugger-exceptionBreakpointsCollapsed": false
    hyperclick:
      darwinTriggerKeys: "shiftKey,metaKey"
    use:
      "atom-ide-busy-signal": "default"
      "atom-ide-code-actions": "default"
      "atom-ide-code-format": "default"
      "atom-ide-code-highlight": "default"
      "atom-ide-console": "default"
      "atom-ide-datatip": "default"
      "atom-ide-debugger": "default"
      "atom-ide-definitions": "default"
      "atom-ide-diagnostics": "default"
      "atom-ide-diagnostics-ui": "always"
      "atom-ide-find-references": "default"
      "atom-ide-global": "default"
      "atom-ide-outline-view": "default"
      "atom-ide-signature-help": "default"
      "atom-ide-terminal": "default"
      hyperclick: "default"
  "atom-package-deps":
    ignored: []
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "symbols-view"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "mariana-syntax"
    ]
  editor:
    fontSize: 12
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapHangingIndent: 4
    tabLength: 4
    zoomFontWhenCtrlScrolling: true
  "exception-reporting":
    userId: "3b434e5e-6084-43b4-bf52-9ce23617420a"
  "fold-comments":
    autofold: true
  "ftp-remote-edit":
    config: ""
    finder:
      filterKeyForItem: "Path"
    password: "93ab036c58cd9d88"
    tree:
      showOnRightSide: false
  "ide-php":
    phpPath: "/usr/local/opt/php/bin/php"
  "linter-js-standard":
    lintMarkdownFiles: true
    showEslintRules: true
    style: "semistandard"
  "linter-ui-default":
    panelHeight: 300
    showPanel: true
  minimap:
    adjustMinimapWidthOnlyIfSmaller: false
    displayPluginsControls: false
    moveCursorOnMinimapClick: true
    scrollAnimation: true
  "one-dark-ui": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "php-cs-fixer":
    executablePath: "/usr/local/bin/php-cs-fixer"
    executeOnSave: true
    rules: "@PSR2,no_short_echo_tag,indentation_type"
    showInfoNotifications: true
  "php-debug":
    showWelcome: false
  pigments:
    extendAutocompleteToColorValue: true
    extendAutocompleteToVariables: true
    markerType: "native-square-dot"
    mergeColorDuplicates: true
    sassShadeAndTintImplementation: "bourbon"
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      showInStatusBar: true
    prettierEslintOptions:
      prettierLast: true
    prettierOptions:
      jsxBracketSameLine: true
      trailingComma: "es5"
    useEslint: true
  "pretty-json":
    prettifyOnSaveJSON: true
  react: {}
  "standard-formatter":
    checkStyleDevDependencies: true
    formatOnSave: true
    style: "happiness"
  "structure-view":
    AutoscrollFromSource: true
  "symbols-navigator":
    autoRevealOnStart: true
    position: "Right"
  "symbols-view-plus":
    plusConfigurations:
      updateProjectTagsOnFileChange: true
  "tree-view-breadcrumb":
    displayProjectRoot: true
    displayProjectRootName: true
    keepBreadcrumbVisible: true
    pathStyle: true
  "xml-formatter":
    useTab: "true"
".js.regexp.replacement.source":
  editor:
    tabLength: 2
".js.source":
  editor:
    tabType: "soft"
".jsdoc.source":
  editor:
    tabType: "soft"
